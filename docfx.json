{
    "metadata": [
      {
        "src": [
          {
            "files": ["**/*.cs"],
            "exclude": ["**/bin/**", "**/obj/**"],
            "src": "./Scripts/V2/"
          }
        ],
        "dest": "api"
      }
    ],
    "build": {
      "content": [      
        {
          "files": ["docs/**.md", "docs/**/toc.yml", "toc.yml", "*.md"]
        },
        {
          "files": ["Scripts/V2/Data/DB/README_*.md"],
          "dest": "docs/data-system"
        }
      ],
      "resource": [
        {
          "files": ["images/**"]
        }
      ],
      "dest": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": ["default"],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false
    }
  }
